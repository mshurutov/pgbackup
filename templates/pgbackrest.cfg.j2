# {{ pgbackup_config_file }} is config file for pgbackrest
# It is managed by ansible. You should not edit it manually.
[global]
{% for key,value in pgbackup_global_params.items() %}
{{ key | replace("_","-") }}={{ value }}
{% endfor %}
{% for stanza in pgbackup_instances %}

[{{ stanza.name }}]
{% for key1,value1 in stanza.backup_params.items() %}
{{ key1 | replace("_","-") }}={{ value1 }}
{% endfor %}
{% endfor %}
